{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:vue/vue3-essential",
		"prettier",
		"plugin:prettier/recommended"
	],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["vue", "prettier"],
	"rules": {
		"prettier/prettier": "warn"
	}
}
